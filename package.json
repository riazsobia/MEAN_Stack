{
  "name": "MEAN-typescript-seed-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./server/bin/www",
    "build": "del-cli server/public/js/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli ./server/public/js/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'server/public/js/app/**/*.js' 'server/public/js/app/**/*.js.map' '!server/public/js/app/app.js' '!server/public/js/app/*.chunk.js' 'client/app/**/*.ngfactory.ts' 'client/app/**/*.shim.ts' 'client/app/**/*.ngsummary.json' 'client/app/**/*.ngstyle.ts'"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angular/upgrade": "^4.0.0",
    "angular2-cookie": "^1.2.6",
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "errorhandler": "^1.5.0",
    "express": "~4.14.0",
    "hbs": "~3.1.0",
    "jsonwebtoken": "^5.7.0",
    "method-override": "^2.3.9",
    "mongoose": "^4.4.12",
    "mongoose-unique-validator": "^1.0.2",
    "morgan": "~1.6.1",
    "ng2-bootstrap": "1.1.16",
    "reflect-metadata": "^0.1.3",
    "rss-parser": "^2.10.1",
    "rxjs": "^5.2.0",
    "serve-favicon": "~2.3.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.2.12",
    "@types/core-js": "0.9.36",
    "@types/node": "^6.0.45",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "del-cli": "^0.2.0",
    "express-list-endpoints": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.4.4",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.3.0",
    "typescript": "^2.1.4",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.1.0"
  }
}
